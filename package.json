    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "author": "Elliott Foster <elliottf@codebrews.com> (http://codebrews.com/)",
  "license": "MIT",
  "dependencies": {
    "once": "1.4.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.6",
    "eslint": "^3.1.0",
    "ghooks": "^1.2.4",
    "istanbul": "^0.4.2",
    "nodeunit": "^0.10.2"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
